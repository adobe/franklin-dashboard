{
  "name": "@adobe/franklin-dashboard",
  "private": true,
  "version": "1.0.0",
  "description": "Franklin statistical dashboard",
  "scripts": {
    "lint:js": "eslint .",
    "lint:css": "stylelint blocks/**/*.css styles/*.css",
    "lint": "npm run lint:js && npm run lint:css",
    "partytown": "npx partytown copylib ./scripts",
    "uglify-partytown": "npx uglify-js ./scripts/partytown*",
    "build": "npm run partytown && npm run uglify-partytown && rm -rf ./scripts/debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobe/helix-project-boilerplate.git"
  },
  "author": "Adobe",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/adobe/franklin-dashboard/issues"
  },
  "homepage": "https://github.com/adobe/franklin-dashboard#readme",
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/eslint-parser": "7.19.1",
    "@builder.io/partytown": "^0.8.0",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@web/test-runner": "0.15.1",
    "@web/test-runner-commands": "0.6.5",
    "chai": "4.3.7",
    "eslint": "8.35.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.27.5",
    "sinon": "15.0.1",
    "stylelint": "15.2.0",
    "stylelint-config-standard": "30.0.1",
    "uglify-js": "^3.17.4"
  }
}
